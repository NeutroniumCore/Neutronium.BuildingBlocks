{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "..\\Neutronium.BuildingBlocks.ApplicationTools"
        }
      ],
      "dest": "applicationTools",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties": { "TargetFramework": "net472" }
    },
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "..\\Neutronium.BuildingBlocks.Application"
        }
      ],
      "dest": "application",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties": { "TargetFramework": "net472" }
    },
  ],
  "build": {
    "content": [
      {
        "files": [
          "application/**.yml",
          "application/index.md"
        ]
      },
      {
        "files": [
          "applicationTools/**.yml",
          "applicationTools/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "..\\docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}